Cell[BoxData[
 RowBox[{
  RowBox[{"edgesTimeSpan", "=", 
   RowBox[{
    RowBox[{
     RowBox[{"Reap", "[", 
      RowBox[{"For", "[", 
       RowBox[{
        RowBox[{"i", "=", "1"}], ",", 
        RowBox[{"i", "\[LessEqual]", 
         RowBox[{"Length", "[", "edges", "]"}]}], ",", 
        RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"edge", "=", 
          RowBox[{"edges", "[", 
           RowBox[{"[", "i", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"teacher", "=", 
          RowBox[{"edge", "[", 
           RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"student", "=", 
          RowBox[{"edge", "[", 
           RowBox[{"[", "2", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"datesTeacher", "=", 
          RowBox[{"transmitterToDates", "[", "teacher", "]"}]}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"datesStudent", "=", 
          RowBox[{"transmitterToDates", "[", "student", "]"}]}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"bdTeacher", "=", 
          RowBox[{"datesTeacher", "[", 
           RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"ddTeacher", "=", 
          RowBox[{"datesTeacher", "[", 
           RowBox[{"[", "2", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"bdStudent", "=", 
          RowBox[{"datesStudent", "[", 
           RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"ddStudent", "=", 
          RowBox[{"datesStudent", "[", 
           RowBox[{"[", "2", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{"bdStudent", ">", "ddTeacher"}], ",", 
           RowBox[{
            RowBox[{"count", "++"}], ";", 
            RowBox[{"Continue", "[", "]"}], ";"}]}], "]"}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"CHILDHOODCONSTANT", "=", "20"}], ";", "\[IndentingNewLine]", 
         RowBox[{"HIJRACONSTANT", "=", 
          RowBox[{"-", "11"}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"startYear", "=", 
          RowBox[{"Max", "[", 
           RowBox[{"HIJRACONSTANT", ",", 
            RowBox[{"bdStudent", "+", "CHILDHOODCONSTANT"}], ",", 
            RowBox[{"bdTeacher", "+", "CHILDHOODCONSTANT"}]}], "]"}]}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"endYear", "=", 
          RowBox[{"Min", "[", 
           RowBox[{"ddTeacher", ",", "ddStudent"}], "]"}]}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{"startYear", ">", "endYear"}], ",", "\[IndentingNewLine]", 
           
           RowBox[{
